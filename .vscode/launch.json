    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome (no web security)",
            "url": "http://localhost:4200",
            "runtimeArgs": [
                "--disable-web-security",
                "--user-data-dir /users/whitney/Dev/Chrome"
            ],
            "webRoot": "${workspaceFolder}"
        }
    ]
}